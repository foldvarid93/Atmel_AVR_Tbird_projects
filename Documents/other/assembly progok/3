.include "m128def.inc"
.org 0x00

.def a=r16
.def b=r17
.def c=r18
.def d=r19
.def e=r20
/// stack létrehozása-->KI kell jelölni a tetejét és alját mivel memórai terület
.macro STACK
ldi	a,		high(ramend)
out	sph,	a    
ldi a,		low(ramend)
out spl,	a
.endmacro

.macro portinit
ldi a,		0x00
sts ddrg.	a
.endmacro





rjmp start

.org 0x100



start:
STACK
portinit 
ldi 
